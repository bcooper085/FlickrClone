@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using System.Security.Claims
@model IEnumerable<FlickrClone.Models.Post>


    @foreach (var post in Model)
    {
        <div class="col-md-4"><h3>@Html.ActionLink(post.Title, "Details", "Posts", new { id = post.PostId }, null)</h3><img src="@Url.Content(post.Image)" alt="Image" /></div>
        
    }

